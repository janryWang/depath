  "homepage": "https://github.com/janryWang/cool-path#readme",
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.1.4",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "jest-codemods": "^0.20.0",
    "lint-staged": "^4.3.0",
    "prettier": "^1.11.x",
